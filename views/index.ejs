			$("form").submit(function (e) {
				e.preventDefault(); // prevents page reloading
				var searchStr = document.getElementById("searchBarInput").value;
				console.log(searchStr);

				$.ajax({
					url: "/company-search",
					dataType: "json",
					method: "POST",
					data: { data: searchStr },
					success: function (returnData) {
						console.log("Data returned from app js: ");
						upDateEJS(returnData);
					},
					error: function (err) {
						console.log(err.status);
					},
				});
			});
			//----------------------------------------------------
			function upDateEJS(returnData) {
				console.log(returnData);

