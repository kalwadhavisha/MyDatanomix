					source: function (req, res) {
						//console.log("Rx data from searchBar:");
						//console.log(req);
						var sendData = req;
						$.ajax({
							url: "/autocomplete/",
							dataType: "jsonp",
							method: "GET",
							data: sendData,
							success: function (data) {
								console.log(data);
								res(data);
							},
							error: function (err) {
								console.log(err.status);
							},
						});
					},
					autoFocus: true,
					delay: 300,
					minLength: 1,
					select: function (event, ui) {
						if (ui.items) {
							$("#searchBar").val(ui.item.label);
							console.log("Rx data from searchBar:");
							console.log(ui.item.label);
							//console.log(ui.item.sym);
						}
					},
				});
